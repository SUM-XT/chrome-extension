{
  "manifest_version":  2,
  "name": "Test1",
  "version": "1.0.0",
  "description": "Learning building chrome ext",
  "icons": {
    "128": "images/Sum_128.png",
    "48": "images/Sum_48.png",
    "16": "images/Sum_16.png"
  },
  "browser_action": {
    "default_icon": "images/Sum_19.png",
    "default_popup": "popup.htm"
  },


  "background": {
    "scripts": ["js/jquery-3.5.1.min.js","js/eventPage.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-3.5.1.min.js", "js/selection.js"]
    }
  ],

  "permissions": [
    "storage",
    "activeTab",
    "notifications",
    "contextMenus"
  ]

}
